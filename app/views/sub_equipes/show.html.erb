<form action="" class="form-horizontal">

  <div class="form-title">
    Informações
  </div>

  <div class="form-group">
    <label class="col-lg-1 control-label">Descrição</label>
    <div class="col-lg-11">
      <input type="text" class='form-control' disabled value="<%=	 @sub_equipe.descricao  %>">
    </div>
  </div>

  <div class="form-group">
    <label class="col-lg-1 control-label">Equipe</label>
    <div class="col-lg-11">
      <input type="text" class='form-control' disabled value="<%=	 @sub_equipe.equipe  %>">
    </div>
  </div>



  <%= link_to 'Listar', sub_equipes_path, class: 'btn btn-blue' %>
  <%= link_to 'Editar', edit_sub_equipe_path(@sub_equipe), class: 'btn btn-blue' %>

</form>